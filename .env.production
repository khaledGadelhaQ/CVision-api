# Production Environment Configuration
NODE_ENV=production
PORT=3000

# Database Configuration
DB_HOST=your-prod-db-host
DB_PORT=5432
DB_USERNAME=prod_user
DB_PASSWORD=your-secure-prod-password
DB_DATABASE=cvision_prod

# API Configuration
API_PREFIX=api
API_VERSION=v1

# Security
JWT_SECRET=your-super-secure-prod-jwt-secret-key
JWT_EXPIRES_IN=1h

# External Services
AI_MODEL_API_URL=https://your-prod-ai-api.com
AI_MODEL_API_KEY=your-prod-api-key

# Logging
LOG_LEVEL=error
ENABLE_REQUEST_LOGGING=false

# Performance
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
